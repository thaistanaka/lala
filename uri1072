#include <stdio.h>
#include <stdlib.h>

int main()
{
    int N,i,X,in,out;
    scanf("%i",&N);
    in=0;
    out=0;
    if(N<10000){
       for(i=1;i<=N;i++) {
        scanf("%i",&X);
        if(X>=10&&X<=20)
            in=in+1;
        else
            out=out+1;
       }
       printf("%i in\n",in);
       printf("%i out\n",out);
    }
    return 0;
}
