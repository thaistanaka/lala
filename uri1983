#include <stdio.h>
#include <stdlib.h>

int main()
{
    int n,m,i;
    int y;
    float nota,z;
    scanf("%i",&n);
    z=0;
    y=0;
    for(i=1;i<=n;i++){
        scanf("%i",&m);
        scanf("%f",&nota);
        if(nota>z){
            z=nota;
            y=m;
            }
    }
    if(z<8)
        printf("Minimum note not reached\n");
        else
            printf("%i\n",y);
    return 0;
}
