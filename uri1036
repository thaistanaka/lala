#include <stdio.h>
#include <stdlib.h>

int main()
{
    double A,B,C,r1,r2;
    scanf("%lf %lf %lf",&A,&B,&C);
    if(A!=0&&(B*B-4*A*C)>0){
        r1=(-B+sqrt(B*B-4*A*C))/(2*A);
        r2=(-B-sqrt(B*B-4*A*C))/(2*A);
        printf("R1 = %.5f\n", r1);
        printf("R2 = %.5f\n", r2);}
        else
            printf("Impossivel calcular\n");
    return 0;
}
