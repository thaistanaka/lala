#include <stdio.h>
#include <stdlib.h>

int main()
{
    int x,y,i,j=0;
    scanf("%i\n", &x);
    scanf("%i", &y);
    if(x>y){
        for(i=y;i<=x;i++){
            if(i%13!=0)
               j=j+i;
        }
    }
    else {
        for(i=x;i<=y;i++){
            if(i%13!=0)
               j=j+i;
    }}
    printf("%i\n",j);
    
    return 0;
}
