#include<stdio.h>
#include<stdlib.h>
int main(){
    double  M[12][12],soma,x;
    int i,j;
    char oper;
    scanf("%c\n",&oper);
    for(i=0;i<12;i++){
        for(j=0;j<12;j++){
            scanf("%lf", &M[i][j]);
        }
    }
    soma=0.0;
    for(i=0;i<12;i++){
    for(j=0;j<12;j++){
        if((i+j)<11)
        soma=soma+M[i][j];
    }}
    if(oper=='M')
        x=soma/66.0;
        else
        x=soma;
        printf("%0.1f\n",x);
    return 0;
}
